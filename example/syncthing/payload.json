{
  "templates_dir": "../example/syncthing/templates/",
  "app_context": {
    "name": "syncthing",
    "version": "1.2.3",
    "app_dataset": "../example/syncthing/files",
    "operation": "Create",
    "upgrade_metadata": {
      "from_version": "1.2.2",
      "to_version": "1.2.3"
    },
    "system": {
      "scale_version": "1.2.3"
    }
  },
  "values": {
    "syncthingConfig": {
      "additionalEnvs": [],
      "announceLANAddresses": false,
      "globalDiscovery": false,
      "localDiscovery": false,
      "natTraversal": false,
      "relaying": false,
      "telemetry": false
    },
    "resources": {
      "limits": {
        "cpu": "4.0",
        "memory": "8gb"
      }
    },
    "syncthingID": {
      "group": 568,
      "user": 568
    },
    "syncthingNetwork": {
      "certificateID": "1",
      "hostNetwork": true,
      "localDiscoveryPort": 21027,
      "quicPort": 22000,
      "tcpPort": 22000,
      "webPort": 31000
    },
    "TZ": "Europe/Athens",
    "syncthingStorage": {
      "additionalStorages": [
        {
          "hostPathConfig": {
            "aclEnable": false,
            "hostPath": "/tmp/test"
          },
          "mountPath": "/data1",
          "readOnly": false,
          "type": "hostPath"
        }
      ],
      "home": {
        "type": "ixVolume",
        "hostPathConfig": {
          "aclEnable": false,
          "hostPath": "/tmp/test1"
        }
      }
    }
  }
}
